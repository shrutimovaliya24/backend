{
  "version": 2,
  "functions": {
    "api/webhook.js": {
      "maxDuration": 30,
      "runtime": "nodejs18.x",
      "handler": "wix-webhook-server/server.js"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}
